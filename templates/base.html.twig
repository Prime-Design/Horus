<!DOCTYPE html>
<html lang="{{ app.request.locale }}">

	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="description" content="{% block meta_description %}Site pour avoir des infos sur des concerts programmés{% endblock %}">
		<meta name="robots" content="index, follow">

		<script src="{{ asset('tarteaucitron/tarteaucitron.js') }}"></script>

		<script type="text/javascript">
			document.addEventListener("DOMContentLoaded", function () {
tarteaucitron.init({ // Configurations pour tarteaucitron
});

tarteaucitron.job = tarteaucitron.job || [];
tarteaucitron.job.push('gtag', 'facebook', 'instagram');
});
		</script>

		<title>
			{% block title %}Programmation| Réservation de concerts
			{% endblock %}
		</title>

		<link href="https://fonts.cdnfonts.com/css/poppins" rel="stylesheet">
		<link rel="preconnect" href="https://fonts.cdnfonts.com/" crossorigin>
		<link rel="icon" type="image/x-icon" href="{{ asset('divers/images/logo.png') }}">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg==" crossorigin="anonymous" referrerpolicy="no-referrer"/> {% block stylesheets %}{% endblock %}
<link href="/assets/styles/css/footer.css" rel="stylesheet">

<link href="/assets/styles/css/header.css" rel="stylesheet">


<link href="/assets/styles/css/header2.css" rel="stylesheet">


<link href="/assets/styles/css/index.css" rel="stylesheet">


<link href="/assets/styles/css/login.css" rel="stylesheet">


<link href="/assets/styles/css/register.css" rel="stylesheet">


<link href="/assets/styles/css/rubrik.css" rel="stylesheet">


<link href="/assets/styles/css/show.css" rel="stylesheet"

<link href="/assets/styles/css/rgpd.css" rel="stylesheet"





		{% block javascripts %}

{% endblock %}
	</head>


	<body>
		{{ include('elements/header.html.twig') }}
		{% block body %}{% endblock %}
		{{ include('elements/footer.html.twig') }}
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
		<script>
const hamburgerButton = document.querySelector(".nav-toggler");
const navigation = document.querySelector("nav");

hamburgerButton ?. addEventListener("click", toggleNav);

function toggleNav() {
hamburgerButton.classList.toggle("active");
navigation.classList.toggle("active");
}
		</script>
	</body>
</html>
